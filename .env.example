# ePrescription - Environment Variables Template
# Copy this file to .env and fill in your actual values
# NEVER commit .env file to git!

# ============================================
# ORACLE DATABASE
# ============================================
ORACLE_PWD=YOUR_ORACLE_PASSWORD_HERE
EPRESCRIPTION_DB_PASSWORD=YOUR_DB_PASSWORD_HERE
KEYCLOAK_DB_PASSWORD=YOUR_KEYCLOAK_DB_PASSWORD_HERE

# ============================================
# KEYCLOAK
# ============================================
KEYCLOAK_ADMIN_PASSWORD=YOUR_ADMIN_PASSWORD_HERE
KEYCLOAK_CLIENT_SECRET=YOUR_CLIENT_SECRET_HERE

# ============================================
# EXTERNAL APIs
# ============================================

# Hugging Face API (AI Analysis)
HUGGINGFACE_API_KEY=YOUR_HUGGINGFACE_API_KEY_HERE

# WHO API (Official ICD-10 Catalog)
WHO_API_CLIENT_ID=YOUR_WHO_CLIENT_ID_HERE
WHO_API_CLIENT_SECRET=YOUR_WHO_CLIENT_SECRET_HERE
WHO_API_BASE_URL=https://icd.who.int/icdapi

# DeepL Translation Service
DEEPL_API_KEY=YOUR_DEEPL_API_KEY_HERE
DEEPL_API_ENDPOINT=https://api-free.deepl.com/v2

# ============================================
# NOTES
# ============================================
# 1. Copy this file: cp .env.example .env
# 2. Fill in your actual values in .env
# 3. .env is in .gitignore and will NOT be committed
# 4. Keep this .env.example file updated with new variables
# 5. DO NOT put real secrets in this file!
